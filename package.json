{
  "name": "@lang-tag/cli",
  "version": "0.16.0",
  "type": "module",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheTonsOfCode/lang-tag-cli"
  },
  "author": "TheTonsOfCode",
  "license": "MIT",
  "bin": {
    "lang-tag": "./index.js",
    "langtag": "./index.js"
  },
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./algorithms": {
      "types": "./algorithms/index.d.ts",
      "import": "./algorithms/index.js",
      "require": "./algorithms/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "./build.sh",
    "publish": "./publish.sh",
    "pack-test-build": "./pack-test-build.sh",
    "test": "vitest --run --exclude '**/e2e/**'",
    "test-full": "./pack-test-build.sh && TESTS_BY_COMMAND=true vitest --run",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "husky"
  },
  "peerDependencies": {
    "lang-tag": "*"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "case": "^1.6.3",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "globby": "^14.1.0",
    "json5": "^2.2.3",
    "micromatch": "^4.0.8",
    "mustache": "^4.2.0",
    "pathe": "^2.0.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/micromatch": "^4.0.9",
    "@types/mustache": "^4.2.6",
    "@types/node": "^22.13.13",
    "@types/react": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "anymatch": "^3.1.3",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lang-tag": "^0.10.1",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.9"
  },
  "keywords": [
    "i18n",
    "translation",
    "internationalization",
    "localization",
    "typescript",
    "react",
    "component-based",
    "translation-management",
    "javascript",
    "lang-tag",
    "jsx",
    "tsx",
    "inline-translations",
    "translation-keys",
    "component-translations",
    "translation-extraction",
    "namespaces",
    "type-safe-translations",
    "i18n-automation",
    "translation-tags",
    "language-tags",
    "translation-structures",
    "collected-translations",
    "translation-organization",
    "translation-library"
  ]
}
